<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <Button
        android:id="@+id/get_data_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="retrofit+okhttp异步请求数据"/>
    <Button
        android:id="@+id/sync_get_data_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="retrofit+okhttp同步请求数据"/>
    <Button
        android:id="@+id/rxjava_retrofit_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="retrofit+okhttp+rxjava请求数据"/>
    <Button
        android:id="@+id/rxjava_retrofit_err_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="retrofit+okhttp+rxjava请求数据错误测试"/>
</LinearLayout>